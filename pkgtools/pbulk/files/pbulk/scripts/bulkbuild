#!@SH@
# $NetBSD: bulkbuild,v 1.1.1.1 2007/06/19 19:49:59 joerg Exp $

if [ "$1" = "-h" ]; then
	echo "usage: $0 [-h] [<config file>]"
	exit 2
elif [ -f "$1" ]; then
	case "$1" in
	/*)
		export PBULK_CONF="$1"; shift
		;;
	*)
		export PBULK_CONF="$(pwd)/$1"; shift
		;;
	esac
fi

. ${PBULK_CONF:-@PBULK_CONFIG@}

set -e

${script_phase_pre_build}
${script_phase_scan}
${script_phase_build}
${script_phase_report}
${script_phase_upload}
